{
  "name": "authentication-microservice",
  "version": "0.0.1",
  "main": "dist/index.js",
  "scripts": {
    "start:dev": "yarn prisma:generate && nodemon src/index.ts",
    "test:prod": "yarn prisma:generate && ava --fail-fast",
    "test:dev": "ava --fail-fast --watch",
    "test:dev:e2e": "ava --fail-fast --watch ./src/__tests__/e2e/**",
    "test:dev:integration": "ava --fail-fast --watch ./src/__tests__/integration/**",
    "test:dev:unit": "ava --fail-fast --watch ./src/__tests__/unit/**",
    "build": "tsc",
    "start": "node src/index.js",
    "linter": "yarn eslint ./src/**",
    "prisma:create:migration": "prisma migrate dev",
    "prisma:apply:migration": "prisma migrate deploy",
    "prisma:generate": "prisma generate"
  },
  "repository": "https://github.com/ramonpaolo/authentication-microservice.git",
  "author": "Ramon Paolo Maram <ramonpaolomaran12@gmail.com>",
  "license": "MIT",
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": "dist/"
      },
      "compile": "tsc"
    }
  },
  "devDependencies": {
    "@ava/typescript": "^4.1.0",
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/bcrypt": "^5.0.0",
    "@types/compression": "^1.7.4",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.5",
    "@types/pg": "^8.10.2",
    "@types/sequelize": "^4.28.15",
    "@types/sinon": "^10.0.15",
    "@types/spdy": "^3.4.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "ava": "^5.3.1",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "nodemon": "^2.0.22",
    "prisma": "^5.5.0",
    "sinon": "^15.2.0",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "*"
  },
  "dependencies": {
    "@prisma/client": "^5.5.0",
    "@sendgrid/mail": "^7.7.0",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "elasticsearch-transport": "^0.0.3",
    "cors": "^2.8.5",
    "dd-trace": "^4.7.0",
    "express": "^4.18.2",
    "helmet": "6.0.0",
    "ioredis": "^5.3.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.11.0",
    "sequelize": "^6.32.0",
    "sequelize-cli": "^6.6.1",
    "spdy": "^4.0.2",
    "twilio": "^4.12.0",
    "winston": "^3.9.0"
  },
  "engines": {
    "node": "20.8.1"
  }
}